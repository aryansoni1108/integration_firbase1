<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.diegodobelo.expandinganimlib">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:roundIcon="@drawable/material_drawer_circle_mask"
        android:theme="@style/MaterialDrawerTheme.TranslucentStatus">
        android:theme="@style/AppTheme">
        <activity android:name=".AllClasses">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".AllClassesAdapter" />
        <activity android:name=".CrossfadeDrawerLayout"></activity>
    </application>

</manifest>